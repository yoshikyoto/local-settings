[core]
	editor = emacs
	excludesfile = ~/.gitignore_global
	[color]
	ui = true
[user]
	name = yoshikyoto
	email = stb_yoshiyuki-s@hotmail.co.jp
[alias]
	s = status
	cm = commit
	cmm = commit -m
	co = checkout
	coh = checkout HEAD
	b = branch
	cb = checkout -b
	cob = checkout -b
	sm = submodule
	p = push
	d = diff
	dc = diff --cached
	ds = "!f() { git diff HEAD..stash@{$1}; };f"
	sl = stash list
	sp = "!f() { if [ $# -eq 0 ]; then git stash pop; else git stash pop stash@{$1}; fi; };f"
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	stage = add
	unstage = reset HEAD